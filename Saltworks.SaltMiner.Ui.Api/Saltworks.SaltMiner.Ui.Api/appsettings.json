  "LogConfig": {
    "Serilog": {
      "Using": [
        "Serilog.Sinks.Console",
        "Serilog.Sinks.File"
      ],
      "MinimumLevel": {
        "Default": "Debug",
        "Override": {
          "Microsoft": "Warning",
          "System.Net": "Warning",
          "Saltworks.Utility.ApiHelper.ApiClient": "Warning",
          "ApiHelper": "Warning"
        }
      },
      "WriteTo": [
        {
          "Name": "Console",
          "Args": {
            "outputTemplate": "[{Timestamp:g} {Level:u3}] [{SourceContext}] {Message:lj}{NewLine}",
            "restrictedToMinimumLevel": "Information"
          }
        },
        {
          "Name": "Logger",
          "Args": {
            "configureLogger": {
              "Filter": [
                {
                  "Name": "ByExcluding",
                  "Args": {
                    "expression": "Contains(@m, \u0027[Progress\u0027)"
                  }
                }
              ],
              "WriteTo": [
                {
                  "Name": "File",
                  "Args": {
                    "path": "smuiapi-.log",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": "7",
                    "formatter": "Elastic.CommonSchema.Serilog.EcsTextFormatter, Elastic.CommonSchema.Serilog",
                    "restrictedToMinimumLevel": "Debug"
                  }
                }
              ]
            }
          }
        },
        {
          "Name": "Logger",
          "Args": {
            "configureLogger": {
              "Filter": [
                {
                  "Name": "ByIncludingOnly",
                  "Args": {
                    "expression": "Contains(@m, \u0027[Progress\u0027)"
                  }
                }
              ],
              "WriteTo": [
                {
                  "Name": "File",
                  "Args": {
                    "path": "smuiapi-metric-.log",
                    "rollingInterval": "Day",
                    "retainedFileCountLimit": "7",
                    "formatter": "Elastic.CommonSchema.Serilog.EcsTextFormatter, Elastic.CommonSchema.Serilog"
                  }
                }
              ]
            }
          }
        }
      ],
      "Enrich": [
        "FromLogContext",
        "WithExceptionDetails",
        "WithThreadId"
      ],
      "Properties": {
        "Application": "Manager"
      }
    }
  }
}